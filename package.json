{
  "name": "autonym",
  "version": "0.9.0",
  "description": "A KISS JSON REST API framework that can be mounted to your Express application.",
  "keywords": ["autonym", "kiss", "json", "rest", "api", "express", "middleware", "schema", "es6"],
  "main": "lib/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "mocha --require babel-register",
    "build": "rm -rf lib && babel src --out-dir lib --source-maps --copy-files",
    "watch": "npm run build -- --watch",
    "prettier": "prettier --write '{src/**/*.{js,json},test/**/*.js,*.json}'",
    "lint": "eslint src test"
  },
  "lint-staged": {
    "*.{js,json}": ["prettier --write", "git add"],
    "{src,test}/*.{js,json}": ["eslint", "git add"],
    "README.md": ["markdown-toc -i", "git add"]
  },
  "engines": {
    "node": ">=4.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmiller42/autonym.git"
  },
  "author": "Matt Miller <me@mmiller.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmiller42/autonym/issues"
  },
  "homepage": "https://github.com/mmiller42/autonym#readme",
  "devDependencies": {
    "assertion-error": "^1.0.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "body-parser": "^1.18.1",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "express": ">=4.0.0 <6.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "markdown-toc": "^1.1.0",
    "mocha": "^3.4.2",
    "prettier": "^1.6.1",
    "rimraf": "^2.6.1"
  },
  "peerDependencies": {
    "express": ">=4.0.0 <6.0.0",
    "body-parser": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "ajv": "^5.2.2",
    "async-boolean-expression-evaluator": "^1.1.1",
    "eslint-config-prettier": "^2.4.0",
    "http-status-codes": "^1.3.0",
    "inflection": "^1.12.0",
    "instance-of-name": "^0.1.1",
    "lodash": "^4.17.4",
    "maybe-promise-factory": "^1.0.0"
  }
}
